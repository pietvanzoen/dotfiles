#!/usr/bin/env node
// Fetches the URL for a given gif path

const prop = process.argv[2];
const gifPath = process.argv[3];

const MANIFEST_PATH =
  `${process.env.HOME}/Library/Caches/com.runningwithcrayons.Alfred/Workflow Data/com.piet.gifme/manifest.json`;

const manifest = require(MANIFEST_PATH);

async function main() {
  console.log(manifest[gifPath][prop]);
}

main();
